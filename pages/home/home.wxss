page {
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
}

.map-view {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.map-view >view {
  height: 84rpx;
  background-color: #fff;
  position: relative;
}

.search-block {
  width: 666rpx;
  height: 60rpx;
  background: #f8f8f9;
  border-radius: 200rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  display: flex;
  align-items: center;
}

.search-block  >text {
  font-size: 26rpx;
  color: #9b9b9b;
  margin-left: 8rpx;
}

.search-center {
  justify-content: center;
}

.search-left  >text {
  flex: 1;
}

.clear {
  margin-right: 25rpx;
}

#myMap {
  width: 100%;
  flex: 1;
}

.search-icon {
  position: absolute;
  top: 0;
  right: 0;
}

.activity-guide {
  width: 91.4%;
  height: 106rpx;
  position: absolute;
  text-align: center;
  left: 0;
  right: 0;
  top: 30rpx;
  margin: auto;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 34rpx;
  color: #262626;
}

.activity-guide .scroll-content {
  margin-left: 20rpx;
}

.activity-guide cover-view cover-image {
  margin-right: 15rpx;
  margin-left: 10rpx;
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
}

.tipNotice-text {
  font-size: 30rpx;
  color: #777;
  text-align: left;
  flex: 1;
  width: 480rpx;
}

.tip-notice-arrow {
  width: 20rpx;
  height: 22rpx;
  margin-right: 30rpx;
}

.position {
  position: absolute;
  bottom: 179rpx;
  width: 88rpx;
  height: 88rpx;
}

.scan {
  width: 678rpx;
  height: 98rpx;
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  bottom: 55rpx;
}

.home-anchor {
  height: 78rpx;
  width: 40rpx;
  position: absolute;
  margin: auto;
  right: 0;
  left: 0;
  top: 40%;
}

.ordering {
  width: 206rpx;
  height: 106rpx;
  position: absolute;
  right: 0;
  top: 230rpx;
  transform: translateX(1000px);
}

.charge-detail {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  height: 360rpx;
  padding: 36rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  transform: translateY(1000px);
}

.wrap-width {
  text-align: center;
  width: 200rpx;
}

.charge-detail .detail-up {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.charge-detail .detail-up .detail-up-right {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 35rpx;
}

.address {
  font-size: 32rpx;
  color: #3d3d3d;
  font-weight: bold;
  flex: 1;
}

.charge-detail .detail-up .detail-up-right cover-image {
  width: 38rpx;
  height: 40rpx;
}

.charge-detail .detail-up .detail-up-right cover-view {
  color: #9b9b9b;
  font-size: 24rpx;
}

.charge-detail .detail-center {
  display: flex;
  align-items: center;
  justify-content: space-between;
  transform: translateY(-10rpx);
}

.charge-detail .detail-center .detail-center-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.detail-center-item-wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #9b9b9b;
}

.detail-center-item-wrap:last-child {
  margin-top: 10rpx;
}

.number {
  font-size: 32rpx;
  color: #000;
  padding: 0 4rpx;
  text-align: center;
}

.price-number {
  font-size: 32rpx;
  color: #000;
  padding: 0 4rpx;
}

.price {
  /* width: 200rpx; *//* background: #787878; */
}

.detail-bottom {
  display: flex;
  justify-content: space-between;
}

.scan-small-icon {
  width: 420rpx;
}

.detail-icon {
  width: 222rpx;
}

.distance {
  text-align: center;
  width: 140rpx;
}
